{
  "name": "cursor-prototype-launcher",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "install:prototype": "npm run install:onboarding",
    "install:onboarding": "npm --prefix projects/ai-playground-onboarding install",
    "install:starter": "npm --prefix projects/ai-playground-starter install",
    "install:all": "npm run install:onboarding && npm run install:starter",
    "dev": "npm run dev:onboarding",
    "dev:onboarding": "npm --prefix projects/ai-playground-onboarding run dev",
    "dev:starter": "npm --prefix projects/ai-playground-starter run dev",
    "build": "npm run build:onboarding",
    "build:onboarding": "npm --prefix projects/ai-playground-onboarding run build",
    "build:starter": "npm --prefix projects/ai-playground-starter run build",
    "build:all": "npm run build:onboarding && npm run build:starter",
    "preview": "npm run preview:onboarding",
    "preview:onboarding": "npm --prefix projects/ai-playground-onboarding run preview",
    "preview:starter": "npm --prefix projects/ai-playground-starter run preview",
    "check": "npm run check:onboarding",
    "check:onboarding": "npm --prefix projects/ai-playground-onboarding run check",
    "check:starter": "npm --prefix projects/ai-playground-starter run check",
    "check:all": "npm run check:onboarding && npm run check:starter",
    "connect:design-system": "npm --prefix projects/ai-playground-onboarding run connect:design-system",
    "sync:design-tokens": "npm --prefix projects/ai-playground-onboarding run sync:design-tokens"
  }
}
